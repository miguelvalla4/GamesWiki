{
    "name": "freepikcompany/backend-coding-task-slim",
    "description": "A Slim implementation of FC's Backend coding task",
    "license": "MIT",
    "authors": [
        {
            "name": "Jos√© Antonio Muriano Criado",
            "email": "jamuriano@freepik.com"
        },
        {
            "name": "Juan Manuel Cabello Becerro",
            "email": "jcabello@freepik.com"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "ext-json": "*",
        "monolog/monolog": "^2.2",
        "php-di/php-di": "^6.3",
        "slim/psr7": "^1.3",
        "slim/slim": "^4.7",
        "symfony/dotenv": "^5.3"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.30.1",
        "composer/package-versions-deprecated": "^1",
        "infection/infection": "^0.15",
        "jangregor/phpstan-prophecy": "^0.8.1",
        "phpstan/extension-installer": "^1.1.0",
        "phpstan/phpstan": "^0.12.80",
        "phpunit/phpunit": "^8.0 || ^9.0",
        "symfony/var-dumper": "^5.3"
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "test/"
        }
    }
}
